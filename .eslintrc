{
  'parser': 'babel-eslint',
  'extends': 'airbnb',
  'plugins': [
  ],
  'rules': {
    'react/jsx-curly-spacing': [2, 'never'],
    'indent': [2, 2, { 'SwitchCase': 1 }],
    'react/jsx-filename-extension': 0,
    'react/forbid-prop-types': 0,
    'no-param-reassign': 0,
  },
  'globals': {
    'document': true,
    'describe': true,
    'it': true,
    'xit': true,
    'before': true,
    'beforeEach': true,
    'after': true,
    'afterEach': true
  }
}
